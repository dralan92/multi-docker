{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name" : "client",
            "image" : "draln/multi-client",
            "hostname" : "client",
            "essential": false
            
        },
        {
            "name" : "server",
            "image" : "draln/multi-server",
            "hostname":"api",
            "essential": false
            

        },
        {
            "name" : "worker",
            "image" : "draln/multi-worker",
            "hostname":"worker",
            "essential": false
            

        },
        {
            "name" : "nginx",
            "image" : "draln/multi-nginx",
            "hostname":"nginx",
            "essential": true,
            "portMappings" : [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"]
            

        }
    ]
}